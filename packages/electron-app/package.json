{
  "name": "@inabapet/electron-app",
  "type": "module",
  "version": "1.1.2",
  "description": "A cute table pet",
  "author": {
    "name": "Inaba"
  },
  "main": "src/main.ts",
  "build": {
    "appId": "com.inaba.client",
    "productName": "InabaPet",
    "extraResources": [
      {
        "from": "../pet-renderer/dist",
        "to": "renderer"
      }
    ],
    "win": {
      "icon": "assets/icon.ico"
    }
  },
  "scripts": {
    "dev": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "tsc",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-store": "^11.0.2"
  }
}
